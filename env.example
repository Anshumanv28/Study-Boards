# Study Boards Application Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
# Get these from your Supabase project settings
REACT_APP_SUPABASE_URL=https://your-project-ref.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# S3 Storage Configuration (for Supabase Storage)
# Get these from your Supabase Storage settings
REACT_APP_S3_ACCESS_KEY_ID=your-s3-access-key-id
REACT_APP_S3_SECRET_ACCESS_KEY=your-s3-secret-access-key
REACT_APP_S3_ENDPOINT=https://your-project-ref.storage.supabase.co/storage/v1/s3
REACT_APP_S3_REGION=project_region
REACT_APP_S3_BUCKET_NAME=Boards-study

# Application Configuration
REACT_APP_APP_NAME=StudyBoardsMath
REACT_APP_APP_VERSION=1.0.0

# Watermark Configuration
REACT_APP_WATERMARK_TEXT=StudyBoards - Confidential

# Supabase Edge Functions Configuration
# Optional: Override the default Edge Functions URL
# Default is derived from REACT_APP_SUPABASE_URL
REACT_APP_SUPABASE_FUNCTIONS_URL=https://your-project-ref.functions.supabase.co
